    <TextView
        android:id="@+id/textView0"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="112dp"
        android:layout_marginTop="240dp"
        android:background="@drawable/white_background"
        android:padding="5dp"
        android:text="Register Now!"
        android:textColor="@color/black"
        android:textSize="24dp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="120dp"
        android:layout_height="0dp"
        android:layout_marginTop="50dp"
        android:layout_marginBottom="146dp"
        android:background="@drawable/white_background"
        android:padding="5dp"
        android:src="@drawable/login_icon"
        app:layout_constraintBottom_toTopOf="@+id/edt_doc_username"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:layout_editor_absoluteX="150dp"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/edt_doc_username"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:layout_centerVertical="true"
        android:layout_marginBottom="23dp"
        android:background="@drawable/white_background"
        android:gravity="center"
        android:hint="Username"
        android:padding="5dp"
        android:textColor="@color/black"
        app:layout_constraintBottom_toTopOf="@+id/edt_doc_email"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView"
        app:layout_editor_absoluteX="25dp"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/edt_doc_email"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:layout_centerVertical="true"
        android:layout_marginBottom="364dp"
        android:background="@drawable/white_background"
        android:gravity="center"
        android:hint="Email"
        android:inputType="textEmailAddress"
        android:padding="5dp"
        android:textColor="@color/black"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/edt_doc_username"
        app:layout_editor_absoluteX="25dp"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/edt_doc_password"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:layout_centerVertical="true"
        android:layout_marginTop="88dp"
        android:background="@drawable/white_background"
        android:gravity="center"
        android:hint="Password"
        android:inputType="textPassword"
        android:padding="5dp"
        android:textColor="@color/black"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/edt_doc_username"
        app:layout_editor_absoluteX="25dp"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/edt_doc_speciality"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:layout_centerVertical="true"
        android:layout_marginTop="156dp"
        android:background="@drawable/white_background"
        android:gravity="center"
        android:hint="Speciality"
        android:inputType="textPassword"
        android:padding="5dp"
        android:textColor="@color/black"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/edt_doc_username"
        app:layout_editor_absoluteX="25dp"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/btn_doc_register"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="112dp"
        android:backgroundTint="@color/white"
        android:padding="5dp"
        android:text="Register"
        android:textColor="@color/black"
        android:textSize="18dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.489"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/edt_doc_password"
        app:layout_editor_absoluteY="450dp" />

    <TextView
        android:id="@+id/docRegister"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:background="@drawable/white_background"
        android:padding="5dp"
        android:text="Already have an account?"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_doc_register"
        app:layout_editor_absoluteX="90dp"
        app:layout_editor_absoluteY="550dp"
        tools:ignore="MissingConstraints" />

</androidx.constraintlayout.widget.ConstraintLayout>